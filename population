import cv2 
vidcap = cv2.VideoCapture('cube.mp4')
success, image = vidcap.read()
count = 0
img = 0
while success:
    success, image = vidcap.read()
    name = "cube_img/frame" + str(count) + ".jpg"
    if img < 90:
        if count % 3.3 < 1:
            cv2.imwrite(name, image)
            img += 1
        count += 1
    else:
        exit(0)
    if cv2.waitKey(10) == 27:  
        break
